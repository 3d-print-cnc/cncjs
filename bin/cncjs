#!/usr/bin/env node

require('core-js/stable'); // to polyfill ECMAScript features
require('regenerator-runtime/runtime'); // needed to use transpiled generator functions

const launchServer = (process.env.NODE_ENV === 'development')
    ? require('../output/server-cli').default
    : require('../dist/cncjs/server-cli').default;

launchServer().catch(err => {
    console.error('Error:', err);
});
